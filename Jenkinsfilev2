pipeline {
  agent any
   stages {
    stage ('Clean') {
      steps {
        sh '''#!/bin/bash
        echo "command to run the pkill.sh"
        '''
     }
   }
 stage ('Deploy') {
  steps {
     sh '''#!/bin/bash
      scp setup.sh ubuntu@107.22.153.95:/home/ubuntu
      ssh ubuntu@107.22.153.95 'bash -s </home/ubuntu/setup2.sh'
    '''
      }
    }
  }
}
